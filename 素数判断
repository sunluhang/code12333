#include <stdio.h>
#include <stdbool.h>//布尔值的头文件（布尔值是编程中专门用来表示逻辑真/假的数据类型。）
bool isPrime(int n){
    if(n<=1){
     return false;
}
    if(n==2){
     return true;
}
    if(n%2==0){
    return false;
}
    for(int i=3;i*i<=n;i+=2){
       //i+=2的含义是偶数全不是素数，所以只需要判断奇数
    if(n%i==0){
        return false;
    }
    }
    return true;}
int main(){
    int T;
    scanf("%d",&T);
    for(int i=0;i<T;i++){
        int n;
        scanf("%d",&n);
        if(isPrime(n)){
            printf("Yes\n");
        }else{
            printf("No\n");
        }
    }
    return 0;
}
