第一个第二个数是1；Fn=Fn-1+Fn-2;
#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    if(n==1||n==2){
        printf("1\n");
        return 0;}
    int a=1,b=1;
    int temp;
    for(int i= 3;i<=n;i++){
        temp=a+b;
        a=b;
        b=temp;
    

    }
    printf("%d",b);
    return 0;
    

}
